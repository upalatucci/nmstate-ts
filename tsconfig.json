{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "lib": [
        "es6", "dom"
    ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": ".",
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "temp",
  },
  "exclude": ["**/node_modules"]
}
